{
  "name": "glushkovds/laravel-kafka-queue",
  "autoload": {
    "psr-4": {
      "LaravelKafka\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelKafka\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    },
    {
      "name": "lukaszaleckas",
      "email": "lukas.zaleckas@hostinger.com"
    },
    {
      "name": "Denis Glushkov",
      "email": "amkarovec@gmail.com",
      "homepage": "https://github.com/glushkovds"
    }
