{
    "name": "lukaszaleckas/laravel-kafka-queue",
    "autoload": {
        "psr-4": {
            "LaravelKafka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelKafka\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "authors": [
        {
            "name": "lukaszaleckas",
            "email": "lukas.zaleckas@hostinger.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-rdkafka": "*",
        "ext-json": "*",
        "laravel/framework": "^8.83|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.0",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelKafka\\KafkaQueueServiceProvider"
            ]
        }
    }
}
